<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="regression-suite" verbose="10" parallel="tests" thread-count="1">

    <test name="regression">
        <classes>
            <class name="signIn.negative.InvalidUser"/>
            <class name="signIn.negative.InvalidPassword"/>
            <class name="signIn.positive.SuccessLogin"/>
            <class name="signIn.positive.SuccessLoginWithLogout"/>
        </classes>
    </test>

</suite>